template<class T>
class Stack
{
    public:
        //Create empty stack
        Stack(int stackCapacity = 10);
        //If stack is empty return true
        bool IsEmpty() const;
        //return top value
        T& Top() const;
        //Insert item into top of the stack
        void Push(const T& item);
        //Delete top of the stack;
        void Pop();
}